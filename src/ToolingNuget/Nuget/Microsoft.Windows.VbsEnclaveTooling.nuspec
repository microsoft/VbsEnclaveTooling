<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata minClientVersion="2.5">
    <id>Microsoft.Windows.VbsEnclaveTooling</id>
    <version>$target_version$</version>
    <title>Vbs Enclave Tooling</title>
    <authors>Microsoft</authors>
    <owners>Microsoft</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Virtual based secure enclave tooling is used to make interacting with vbs enclaves easier for developers. It supports features like code generation between your app and your enclave to marshal into and out of the enclave.</description>
    <tags>vbs enclave virtual secure vbsenclave native nativepackage</tags>
    <license type="file">LICENSE</license>
    <readme>README.md</readme>
    <copyright>Â© Microsoft Corporation. All rights reserved.</copyright>
    <projectUrl>https://github.com/microsoft/VbsEnclaveTooling</projectUrl>
     
    <!-- Dependencies for both HostApp and Enclaves -->
    <dependencies>
        <group targetFramework="native">
            <dependency id="Microsoft.Windows.ImplementationLibrary" version="1.0.240803.1" />
            <dependency id="Microsoft.Windows.SDK.CPP" version="10.0.26100.2454" />
            <dependency id="Microsoft.Windows.SDK.CPP.arm64" version="10.0.26100.2454" />
            <dependency id="Microsoft.Windows.SDK.CPP.x64" version="10.0.26100.2454" />
        </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\..\..\LICENSE"/>
    <file src="$vbsenclavetooling_x64_exe$" target="bin\x64"/>
    <file src="$vbsenclavetooling_arm64_exe$" target="bin\arm64"/>
    <file src="Microsoft.Windows.VbsEnclaveTooling.props" target="build\native"/>
    <file src="Microsoft.Windows.VbsEnclaveTooling.targets" target="build\native"/>
    <file src="..\..\ToolingSharedLibrary\Includes\VbsEnclaveABI\**" target="src\VbsEnclaveABI\" />
    <file src="..\..\VbsEnclaveSDK\src\veil_enclave_lib\**\*.cpp" target="src\VbsEnclaveSDK\veil_enclave" />
    <file src="..\..\VbsEnclaveSDK\src\veil_host_lib\**\*.cpp" target="src\VbsEnclaveSDK\veil_host" />
    <file src="..\..\VbsEnclaveSDK\src\veil_enclave_lib\**\*.h" target="src\VbsEnclaveSDK\veil_enclave" />
    <file src="..\..\VbsEnclaveSDK\src\veil_host_lib\**\*.h" target="src\VbsEnclaveSDK\veil_host" />
    <file src="..\..\VbsEnclaveSDK\src\veil_any_inc\**" target="src\VbsEnclaveSDK\veil_any_inc" />
    <file src="..\..\..\Common\veil_enclave_wil_inc\**" target="src\" />
    <file src="$vbsenclavetooling_cpp_support_x64_lib$" target="lib\native\x64"/>
    <file src="$vbsenclavetooling_cpp_support_arm64_lib$" target="lib\native\arm64"/>
    <file src="$vbsenclavetooling_enclave_x64_lib$" target="lib\native\x64"/>
    <file src="$vbsenclavetooling_enclave_arm64_lib$" target="lib\native\arm64"/>
    <file src="$vbsenclavetooling_host_x64_lib$" target="lib\native\x64"/>
    <file src="$vbsenclavetooling_host_arm64_lib$" target="lib\native\arm64"/>
    <file src="..\..\..\README.md"/>
  </files>
</package>
