enclave 
{
    // Named enum
    enum Color 
    {
        Red = 1,
        Green = 2,
        Blue = 80000
    };

    // Anonymous enum
    enum 
    {
        Nine = 100,
        Ten = 200
    };

    trusted 
    {
        Color TrustedGetColor(
            Color arg1,

            // Use anonymous enum value for array size for this
            // parameter.
            [in] Color arg2[Nine],
            [in, out] Color arg3[5],
            [out] Color arg4[1],

            // count = 1
            [in] Color* arg5,
            [in,out] Color* arg6,
            [out] Color* arg7,

            // count attribute           
            [in] Color* arg8,
            [in, out] Color* arg9,
            [out] Color* arg10,

            // size attribute
            [in] Color* arg11,
            [in, out] Color* arg12,
            [out] Color* arg13,

            // count parameter
            [in] Color* arg14,
            [in, out] Color* arg15,
            [out] Color* arg16,

            // size parameter
            [in] Color* arg17,
            [in, out] Color* arg18,
            [out] Color* arg19,

            size_t arg20,
            size_t arg21
        );

        Color* GetColorPtr();
    };

    untrusted 
    {
        Color UntrustedGetColor(
            Color arg1,

            [in] Color arg2[5],
            [in,out] Color arg3[5],
            [out] Color arg4[1],

            // count = 1
            [in] Color* arg5,
            [in, out] Color* arg6,
            [out] Color* arg7,

            // count attribute           
            [in] Color* arg8,
            [in, out] Color* arg9,
            [out] Color* arg10,

            // size attribute
            [in] Color* arg11,
            [in, out] Color* arg12,
            [out] Color* arg13,

            // count parameter
            [in] Color* arg14,
            [in, out] Color* arg15,
            [out] Color* arg16,

            // size parameter
            [in] Color* arg17,
            [in, out] Color* arg18,
            [out] Color* arg19,

            size_t arg20,
            size_t arg21
        );

        Color* GetColorPtr();
    };
};
